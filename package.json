{
  "name": "todo-app",
  "description": "Another todo App",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "next dev",
    "build": "pnpm cleanup && next build",
    "start": "next start -p 3000",
    "lint": "next lint && prettier --check --ignore-unknown .",
    "lint-fix": "next lint --fix && prettier --write --ignore-unknown .",
    "lint-types": "tsc --noEmit --pretty",
    "lint-staged": "lint-staged",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watchAll",
    "test:snaps": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "prettier-check": "prettier --check --ignore-unknown .",
    "prettier-fix": "prettier --write --ignore-unknown .",
    "cleanup": "rimraf .next .swc out coverage build dist",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@emotion/cache": "11.13.1",
    "@emotion/react": "11.13.3",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.13.0",
    "@hookform/resolvers": "3.9.1",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "6.1.6",
    "@mui/material-nextjs": "6.1.6",
    "axios": "1.7.7",
    "axios-mock-adapter": "^2.1.0",
    "clsx": "2.1.1",
    "html-react-parser": "5.1.18",
    "js-cookie": "3.0.5",
    "just-compare": "2.3.0",
    "just-debounce-it": "3.2.0",
    "just-is-empty": "3.4.1",
    "luxon": "3.5.0",
    "next": "14.2.16",
    "path-to-regexp": "8.2.0",
    "react": "18.3.1",
    "react-device-detect": "2.2.3",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.1",
    "swr": "2.2.5",
    "todo-app": "file:",
    "ts-node": "^10.9.2",
    "zod": "3.23.8",
    "zustand": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "29.5.12",
    "@types/js-cookie": "3.0.6",
    "@types/luxon": "3.4.2",
    "@types/node": "20.17.5",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.16",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-testing-library": "6.2.2",
    "husky": "9.1.6",
    "include-media": "2.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.10",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.5.6",
    "rimraf": "6.0.1",
    "sass": "1.80.6",
    "typescript": "5.3.3"
  }
}
